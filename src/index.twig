{# index.twig #}
{% extends "templates/landingpages/_layout.twig" %}

{% block header %}
  {{ parent() }}
  <!-- build:bundle -->
  <!-- coalesce:css:target -->
  <link href="assets/css/main.css" rel="stylesheet">
  <!-- endcoalesce -->
  <!-- endbuild -->
{% endblock %}

{% block page %}
    <header class="one">
        <h1>{{ title }}</h1>
    </header>

    {% include './templates/modules/_feature.twig' %}

    <p>
        This page is generated by Twig.js using the gulp-twig gulp plugin.
    </p>
    <p>
      Benefits {{ benefits.length }}
    </p>

    <div id="main"></div>
    <div id="search"></div>

    <ul>
        {% for value in benefits %}
            <li>{{ value }}</li>
        {% endfor %}
    </ul>
{% endblock %}
